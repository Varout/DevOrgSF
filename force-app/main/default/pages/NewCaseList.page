<apex:page controller="NewCaseListController">
    
    <apex:pageBlock >
    
    	<apex:repeat value="{! newCases}" var="case">
            <apex:outputLink value="/{! case.Id}">Case Number: {! case.CaseNumber}</apex:outputLink><br />
        </apex:repeat>
    
    </apex:pageBlock>
    
</apex:page>